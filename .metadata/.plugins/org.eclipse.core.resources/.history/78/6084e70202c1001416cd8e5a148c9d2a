package mar2;

/*
This class instantiates an object of the array list type and
uses some of the methods.
*/

import java.util.*;

public class SuperArray 
{
	public static void main(String[] args) 
	{
		ArrayList numbers = new ArrayList();
		
		numbers.add(new Integer(56));
		
		System.out.println("A new ArrayList contains " + numbers.size() + " elements.");
	}
}
